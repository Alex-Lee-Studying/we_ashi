page {
  background-color: #f2f2f2;
}
.container {
  margin: 30rpx;
}
.proWrap {
  padding: 30rpx 40rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 0 0 87rpx 0 rgba(51,51,51,.1);
}
.proWrap .imgWrap image {
  width: 610rpx;
  height: 350rpx;
}
.proWrap .pname {
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
}
.proWrap .info {
  display: flex;
  justify-content: space-between;
  margin: 20rpx 0;
  font-size: 24rpx;
  color: #989898;
}
.proWrap .price {
  font-size: 32rpx;
  color: #033150;
  font-weight: bold;
}

.addressWrap {
  margin: 30rpx 0;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 0 0 87rpx 0 rgba(51,51,51,.1);
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
  text-align: center;
}

.cellWrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 60rpx;
  margin: 30rpx 0;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #fff;
  border-radius: 20rpx;
  box-shadow: 0 0 87rpx 0 rgba(51,51,51,.1);
  font-size: 28rpx;
  color: #333;
}

.subWrap {
  margin-top: 50rpx;
}
