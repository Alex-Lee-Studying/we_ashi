.expressList .expressItem {
  width: 25%;
  text-align: center;
  margin-top: 30rpx;
}
.expressList .expressItem image {
  margin-bottom: 10rpx;
  width: 60rpx;
}
.expressList .expressItem text {
  display: block;
  font-size: 24rpx;
  color: #333;
}

.networkWrap {
  display: flex;
  margin: 0 35rpx 30rpx;
}

.networkWrap .flag {
  width: 114rpx;
  height: 172rpx;  
  background-color: #fff;
  box-shadow: 0 0 29rpx 0 rgba(51,51,51,.1);
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.networkWrap .flag image {
  width: 55rpx;
}
.networkWrap .flag .country {
  display: block;
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
}
.networkWrap .content {
  flex: 1;
  margin-left: 20rpx;
}
.networkWrap .content .sub{
  padding: 20rpx 25rpx;
  background-color: #fff;
  box-shadow: 0 0 29rpx 0 rgba(51,51,51,.1);
  border-radius: 20rpx;
}
.networkWrap .content .sub+.sub {
  margin-top: 30rpx;
}
.networkWrap .content .other {
  display: block;
  font-size: 20rpx;
  color: #033150;
  line-height: 30rpx;
}
.mb30 {
  margin-bottom: 30rpx;
}