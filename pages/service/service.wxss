.tabWrap {
  display: flex;
  justify-content: space-evenly;
  margin: 20rpx 0 40rpx;
}
.tabWrap .tabItem {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  border-bottom: 6rpx solid #fff;
}
.tabWrap .tabItem.selected {
  color: #033150;
  border-bottom: 6rpx solid #033150;
}
.expressWrap {
  display: flex;
  justify-content: space-between;
  margin: 0 35rpx;
  padding-bottom: 30rpx;
}
.countryWrap {
  width: 95rpx;
  height: 133rpx;  
  background-color: #fff;
  box-shadow: 0 0 29rpx 0 rgba(51,51,51,.1);
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.countryWrap image {
  width: 52rpx;
}
.countryWrap text {
  display: block;
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
}
/* .expressList {
  flex: 1;
  margin-left: 20rpx;
} */
.expressList {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
  background-color: #fff;
  box-shadow: 0 0 29rpx 0 rgba(51,51,51,.1);
  border-radius: 20rpx;
  margin-left: 20rpx;
}
