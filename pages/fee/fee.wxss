.bannerWrap {
  margin: 0 30rpx;
}
.bannerWrap image {
  width: 690rpx;
  height: 300rpx;
  border-radius: 20rpx;
}
.country {
  font-size: 32rpx;
  color: #333;
  text-align: center;
  margin: 50rpx 0 30rpx;
}
.country .iconfont {
  font-size: 28rpx;
  margin: 0 20rpx;
}
.formWrap {
  margin: 0 60rpx;
}

.priceWrap {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  box-shadow: 0 0 29rpx 0 rgba(8,2,4,.07);
}
.priceWrap text {
  margin-left: 30rpx;
  font-size: 24rpx;
  color: #989898;
  font-weight: bold;
}
.priceWrap text.total {
  flex: 1;
  text-align: right;
  font-size: 28rpx;
  color: #033150;
  margin-left: 0;
  margin-right: 60rpx;
}